{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dist",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "target": "ES2020",
    "lib": [
      "es2020",
      "dom"
    ]
  },
  "include": [
    "./src",
    "./test"
  ],
  "declaration": true
}
